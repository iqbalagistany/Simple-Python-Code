        "print(\"PROGRAM BILANGAN FIBONACCI \\n\")\n",
        "\n",
        "user = int(input(\"Masukkan Banyak Bilangan: \"))\n",
        "\n",
        "count = 0\n",
        "x = 0\n",
        "y = 1\n",
        "list_hasil = []\n",
        "\n",
        "while count < user:\n",
        "  list_hasil.append(x)\n",
        "  next = x+y\n",
        "  x = y\n",
        "  y = next\n",
        "  count += 1\n",
        "\n",
        "print(list_hasil)"
