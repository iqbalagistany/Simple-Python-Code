      "source": [
        "n = int(input(\"Masukkan banyak Kolom: \"))\n",
        "\n",
        "for i in range (n):\n",
        "  for j in range(i+1):\n",
        "    print(\"*\", end=\"\")\n",
        "  print(\"\\n\")\n",
        "\n",
        "for n in range (n, 0, -1):\n",
        "  for j in range(n-1):\n",
        "    print(\"*\", end=\"\")\n",
        "  print(\"\\n\")\n",
        "  "
